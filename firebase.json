{
  "hosting": {
    "site": "farefirst-debug-deeplink",
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/deep-link-test",
        "destination": "/deep-link-test.html"
      },
      {
        "source": "/cars.html",
        "destination": "/pages/cars.html"
      },
      {
        "source": "/chauffer.html",
        "destination": "/pages/chauffer.html"
      },
      {
        "source": "/esim.html",
        "destination": "/pages/esim.html"
      },
      {
        "source": "/taxi.html",
        "destination": "/pages/taxi.html"
      },
      {
        "source": "/trips.html",
        "destination": "/pages/trips.html"
      }
    ]
  }
}
